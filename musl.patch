+
+int signbit(double x) {
+	return __signbit(x);
+}
+
+int isfinite(double x) {
+	return (__DOUBLE_BITS(x) & -1ULL>>1) < 0x7ffULL<<52;
+}
+
+int isinf(double x) {
+	return (__DOUBLE_BITS(x) & -1ULL>>1) == 0x7ffULL<<52;
